@model FirstCoreMVCWebApplication.ViewModels.StudentDetailsViewModel
@{
    Layout = null;
}
<html>
<head>
    <title>@Model?.Title</title>
</head>
<body>
    <h1>@Model?.Header</h1>
    <div>
        StudentId : @Model?.Student_Details?.StudentId
    </div>
    <div>
        Name : @Model?.Student_Details?.Name
    </div>
    <div>
        Branch : @Model?.Student_Details?.Branch
    </div>
    <div>
        Section : @Model?.Student_Details?.Section
    </div>
    <div>
        Gender : @Model?.Student_Details?.Gender
    </div>

    <h1>Student Address</h1>
    <div>
        City : @Model?.Student_Address?.City
    </div>
    <div>
        State : @Model?.Student_Address?.State
    </div>
    <div>
        Country : @Model?.Student_Address?.Country
    </div>
    <div>
        Pin : @Model?.Student_Address?.PinCode
    </div>
</body>
</html>